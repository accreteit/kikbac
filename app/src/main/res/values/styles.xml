<resources>
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.NoActionBar"></style>

    <!-- Application theme. -->
    <style name="Theme.Hotline.SelectedTheme" parent="@style/Theme.Hotline.Light.DarkActionBar" >
        <item name="colorPrimary">@color/mainGreen</item>
        <item name="hotlineChannelListItemStyle">@style/Widget.Hotline.ChannelListItem</item>
        <item name="hotlineChannelAltIconStyle">@style/ChannelAltIconCustom</item>

        <item name="hotlineAvatarIcon">@drawable/icon_launcher</item>
        <!--<item name="hotlineContactUsIcon">@drawable/hotline_ic_action_contact_us</item>-->

        <item name="hotlineChatBubbleRight">@drawable/hotline_chat_bubble_right</item>
    </style>

    <style name="ChannelAltIconCustom" parent="Widget.Hotline.ChannelAltIcon">
        <item name="android:layout_height">@dimen/hotline_channel_icon_alt_view_size</item>
        <item name="android:layout_width">@dimen/hotline_channel_icon_alt_view_size</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <!--<item name="android:background">@drawable/hotline_category_alt_text_bg</item>-->
        <item name="android:background">#b00</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:layout_margin">@dimen/hotline_channel_icon_padding</item>
    </style>

    <!--<style name="Theme.Hotline.Light">-->
        <!--<item name="android:background">@color/mainGreen</item>-->
        <!--<item name="hotlineChatBubbleRight">@drawable/hotline_chat_bubble_right</item>-->
    <!--</style>-->

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="colorPrimary">@color/mainGreen</item>
        <item name="colorPrimaryDark">@color/mainGreenDark</item>
        <item name="colorAccent">@color/mainGreen</item>
    </style>


    <style name="BtnGreen">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_green</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45.0dip</item>
    </style>

    <style name="BtnRed">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_red</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45.0dip</item>
    </style>

    <style name="BtnLogin">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_login</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="BtnLoginSocial">
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="BtnLoginSocial.Img" parent="@style/BtnLoginSocial">
        <item name="android:layout_width">50.0dip</item>
        <item name="android:layout_height">50.0dip</item>
    </style>

    <style name="EditOrder">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/textPrimary</item>
        <item name="android:textColorHint">@color/orderText</item>
        <item name="android:background">@drawable/ed_order</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">7.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">7.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="EditEmail">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/textPrimary</item>
        <item name="android:textColorHint">@color/orderText</item>
        <item name="android:background">@drawable/ed_order</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">7.0dp</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">7.0dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="EditOrderLabel">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="FilterSidePadding">
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
    </style>

    <style name="SurveyCheck">
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:button">@drawable/check</item>
    </style>

    <style name="SurveyCheckNew">
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:button">@drawable/check</item>
    </style>

    <style name="ProfileLabel">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/mainGreen</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="ProfileValue">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="NewProfileValue">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="ProfileValue.Radio" parent="@style/ProfileValue">
        <item name="android:paddingLeft">10.0dip</item>
    </style>

    <style name="ProfileValue_Radio_Inline" parent="@style/ProfileValue.Radio">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="Theme.SemiTransparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="ProfileLabel_Inline" parent="@style/ProfileLabel">
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="ProfileValue_Checkbox" parent="@style/ProfileValue">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:paddingBottom">6.0dip</item>
    </style>

    <style name="TabsTheme" parent="@style/Base.ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">@color/mainGreen</item>
        <item name="colorPrimaryDark">@color/mainGreenDark</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="Base.TextAppearance.AppCompat.Body2" parent="@style/Base.TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/abc_text_size_body_2_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="TextAppearance.StatusBar.EventContent.Line2" parent="" />

    <style name="TextAppearance.StatusBar.EventContent" parent="" />

    <style name="TextAppearance.StatusBar.EventContent.Info" parent="" />

    <style name="TextAppearance.StatusBar.EventContent.Time" parent="" />

    <style name="SplashTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorSplashPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorSplashPrimaryDark</item>
        <item name="colorAccent">@color/mainGreen</item>
    </style>

    <style name="expandedappbar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">40.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="collapsedappbar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>